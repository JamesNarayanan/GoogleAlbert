    "manifest_version": 2,
        "name": "Copy Albert",
        "description": "Copies question text in albert.io",
        "version": "1.0",
        "background" : {
            "scripts" : ["background.js"]
        },
        "browser_action": {
            "default_icon": "logo.png"
        },
        "permissions": ["activeTab"]
}